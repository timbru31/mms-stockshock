{
    "name": "stockshock",
    "version": "5.3.0",
    "private": true,
    "description": "A simple bot for MediaMarkt and Saturn to check if items on the wishlist are available",
    "main": "dist/index.js",
    "scripts": {
        "postinstall": "patch-package",
        "build": "tsc",
        "start:dev": "ts-node src/index.ts",
        "prestart": "npm run build",
        "start": "node dist/src/index.js",
        "test": "eslint . --ext .ts",
        "prepare": "husky install"
    },
