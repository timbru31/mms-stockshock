{
    "name": "stockshock",
    "version": "1.2.0",
    "private": true,
    "description": "A simple bot for MediaMarkt and Saturn to check if items on the wishlist are available",
    "main": "dist/index.js",
    "scripts": {
        "postinstall": "patch-package",
        "build": "tsc",
        "start:dev": "ts-node src/index.ts",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "test": "eslint . --ext .ts",
        "prepare": "husky install"
    },
    "repository": "timbru31/mms-stockshock",
    "author": "Tim Brust <github@timbrust.de>",
    "license": " GPL-3.0-or-later",
    "bugs": "https://github.com/timbru31/mms-stockshock/issues",
    "homepage": "https://github.com/timbru31/mms-stockshock#readme",
    "engines": {
        "node": ">=14"
    },
    "dependencies": {
        "@slack/webhook": "^6.0.0",
        "@types/uuid": "^8.3.0",
        "date-fns": "^2.21.2",
        "inquirer": "^8.0.0",
        "patch-package": "^6.4.7",
        "puppeteer": "^9.1.1",
        "puppeteer-extra": "^3.1.18",
        "puppeteer-extra-plugin-stealth": "^2.7.8",
        "toml": "^3.0.0",
        "user-agents": "^1.0.645",
        "uuid": "^8.3.2",
        "winston": "^3.3.3",
        "yargs": "^17.0.1"
    },
    "devDependencies": {
        "@types/inquirer": "^7.3.1",
        "@types/user-agents": "^1.0.1",
        "@types/yargs": "^16.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "eslint": "^7.25.0",
        "husky": "^6.0.0",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.4"
    },
    "funding": "https://github.com/sponsors/timbru31"
}
