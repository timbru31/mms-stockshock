        "winston": "^3.3.3",
        "ws": "^8.2.2",
        "yargs": "^17.1.1"
    },
    "devDependencies": {
        "@types/inquirer": "^8.1.1",
        "@types/user-agents": "^1.0.2",
        "@types/ws": "^7.4.7",
        "@types/yargs": "^17.0.2",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "eslint": "^7.32.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.1.2",
        "prettier": "^2.4.1",
        "ts-node": "^10.2.1",
