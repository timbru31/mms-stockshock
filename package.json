    "bugs": "https://github.com/timbru31/mms-stockshock/issues",
    "homepage": "https://github.com/timbru31/mms-stockshock#readme",
    "engines": {
        "node": ">=16"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.31.0",
        "@types/uuid": "^8.3.1",
        "date-fns": "^2.23.0",
        "discord.js": "^13.1.0",
        "inquirer": "^8.1.5",
        "patch-package": "^6.4.7",
        "proxy-chain": "^1.0.4",
        "puppeteer": "^10.2.0",
        "puppeteer-extra": "^3.1.18",
        "puppeteer-extra-plugin-stealth": "^2.7.10",
        "telegraf": "^4.4.2",
        "toml": "^3.0.0",
        "twitter-api-v2": "^1.5.1",
        "user-agents": "^1.0.776",
